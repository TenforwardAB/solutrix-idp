{
  "name": "solutrix-idp",
  "version": "0.1.0",
  "description": "Solutrix Identity Provider",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "test": "node -e \"console.log('no tests defined')\"",
    "dev": "tsx watch src/server.ts",
    "build": "tsc && rsync -av --exclude='node_modules/' --exclude='.github/' --exclude='tmp/' --exclude='dist/' --exclude='src/models/generated/' --exclude='test.js' --include='*/' --include='*.js' --exclude='*' . dist/",
    "start": "node dist/server.js",
    "migrate": "sequelize db:migrate --config src/config/config.cjs --migrations-path src/migrations",
    "gen:sql": "node scripts/mk-migration.js",
    "gen:models": "node scripts/gen-models.js",
    "docs": "typedoc --options typedoc.json"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "node-jose": "^2.2.0",
    "oidc-provider": "^9.5.2",
    "pg": "^8.12.0",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "wildduck-nodesdk": "^0.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@types/node-jose": "^1.1.13",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "tsx": "^4.19.1",
    "typedoc": "^0.28.14",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  }
}
